{
  "private": true,
  "type": "module",
  "name": "empty-template-project",
  "version": "1.1.0",
  "description": "Repo Template: three, vite, eslint, prettier.",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "browserslist": [
    "Chrome >= 112",
    "ChromeAndroid >= 112",
    "Edge >= 112",
    "Firefox >= 112",
    "FirefoxAndroid >= 112",
    "Safari >= 16.4",
    "iOS >= 16.4",
    "not dead"
  ],
  "eslintConfig": {
    "root": true,
    "extends": "@epreston/eslint-config"
  },
  "prettier": "@epreston/prettier-config",
  "scripts": {
    "dev": "vite",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview",
    "start": "vite --open",
    "lint": "eslint --ext .js --report-unused-disable-directives .",
    "format": "prettier --check --cache \"**/*.[tj]s?(x)\"",
    "format-fix": "prettier --write --cache \"**/*.[tj]s?(x)\""
  },
  "devDependencies": {
    "@epreston/eslint-config": "^1.1.1",
    "@epreston/prettier-config": "^2.0.0",
    "@epreston/tsconfig": "^1.1.1",
    "@tweakpane/core": "^2.0.2",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "vite": "^5.0.10",
    "vite-plugin-glsl": "^1.2.1"
  },
  "dependencies": {
    "@tweakpane/plugin-essentials": "^0.2.0",
    "three": "0.159.0",
    "tweakpane": "^4.0.2"
  },
  "engines": {
    "node": "^18.18.0 || >=20.9.0"
  }
}
