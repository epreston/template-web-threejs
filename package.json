{
  "private": true,
  "type": "module",
  "name": "empty-template-project",
  "version": "1.2.0",
  "description": "Repo Template: three, vite, eslint, prettier.",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "browserslist": [
    "Chrome >= 120",
    "ChromeAndroid >= 120",
    "Edge >= 120",
    "Firefox >= 121",
    "FirefoxAndroid >= 122",
    "Safari >= 17.1",
    "iOS >= 17.1",
    "not dead"
  ],
  "eslintConfig": {
    "root": true,
    "extends": "@epreston/eslint-config",
    "rules": {
      "prettier/prettier": "off"
    }
  },
  "prettier": "@epreston/prettier-config",
  "scripts": {
    "dev": "vite",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview",
    "start": "vite --open",
    "lint": "eslint --ext .js --report-unused-disable-directives .",
    "format": "prettier --check --cache \"**/*.[tj]s?(x)\"",
    "format-fix": "prettier --write --cache \"**/*.[tj]s?(x)\""
  },
  "devDependencies": {
    "@epreston/eslint-config": "^1.1.1",
    "@epreston/prettier-config": "^2.0.0",
    "@epreston/tsconfig": "^1.1.1",
    "@tweakpane/core": "^2.0.4",
    "eslint": "^8.57.0",
    "prettier": "^3.3.2",
    "vite": "^5.3.2",
    "vite-plugin-glsl": "^1.3.0"
  },
  "dependencies": {
    "@tweakpane/plugin-essentials": "^0.2.1",
    "three": "0.166.1",
    "tweakpane": "^4.0.4"
  },
  "engines": {
    "node": "^18.19.0 || >=20.11.0"
  }
}
